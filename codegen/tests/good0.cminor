/*
Arithmetic Expressions
*/

main: function integer ( argc: integer, argv: array [] string ) =
{
	i: integer = 5;
	j: integer = 9;
	k: integer;
	
	k = i+j;
	if (k == 14) {
		print "i + j passed, result is ", k, '\n';
	} else {
		print "i + j failed\n";
	}
	print i-j, ", and the correct result is -4\n";
	print i*j, ", and the correct result is 45\n";
	print i/j, ", and the correct result is 0\n";
	print i%j, ", and the correct result is 5\n";
	print 5^3, ", and the correct result is 125\n";
	print i++, ", and the correct result is 5\n";
	print i, ", and the correct result is 6\n";

	print -j, ", and the correct result is -9\n";
	return 0;
}
