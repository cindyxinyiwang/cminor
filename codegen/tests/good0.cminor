/*
Display fibonnaci numbers from 0 to 45.
This test exercises the ability of your
compiler to call recursive functions.
*/

fib: function integer ( x: integer ) =
{
	if( x<1 ) {
		//print 'i', '\n';
		return 0;
	} else {
		if(x<2) {
			//print 'j', '\n';
			return 1;
		} else {
			//print 'k', '\n';
			return fib(x-1) + fib(x-2);
			//return fib(x-1);
		}
	}
}

