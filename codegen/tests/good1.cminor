/*
Boolean Expressions
*/

main: function integer ( argc: integer, argv: array [] string ) =
{
	i: boolean = true;
	j: boolean = false;
	k: boolean;
	
	print i&&j, ", and the correct result is false\n";
	print i||j, ", and the correct result is true\n";
	print !i, ", and the correct result is false\n";
	print  !false, ", and the correct result is true\n";

	k = i&&!j;
	print k, ", and the correct result is true\n";

	return 0;
}
