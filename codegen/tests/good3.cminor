/*
Complex Boolean Expressions
*/

main: function integer ( argc: integer, argv: array [] string ) =
{
	i: boolean = true;
	j: boolean = false;
	k: boolean;
	
	k = !!!!i && i;
	if (k == true) {
		print "passed, result is ", k, '\n';
	}

	k = i&&j&&!j||i;
	if (k == true) {
		print "passed, result is ", k, '\n';
	}
	return 0;
}
